{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "categories",
  "info": {
    "name": "Category"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "uid"
    },
    "image": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    }
  }
}
