{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "categories",
  "info": {
    "name": "Category"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "class_ids": {
      "collection": "class"
    },
    "userClasses": {
      "via": "classId",
      "collection": "user-class"
    }
  }
}
